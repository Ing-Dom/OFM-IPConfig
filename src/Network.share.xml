<?xml version="1.0" encoding="utf-8"?>
<?xml-model href="../../Organization/knxprod-support/knx_project_20/knx-editor.xsd" type="application/xml" schematypens="http://www.w3.org/2001/XMLSchema"?>
<KNX xmlns:op="http://github.com/OpenKNX/OpenKNXproducer" xmlns="http://knx.org/xml/project/20" CreatedBy="KNX MT" ToolVersion="5.1.255.16695">
  <!-- Enable IPTool of ETS. This hide ip settings in network tab -->
  <op:config name="%NET_IPConfigTool%" value="0" />

  <!-- 
    0 = User select network adapter
    1 = Hide select -> Use LAN
    2 = Hide select -> Use WLAN
  -->
  <op:config name="%NET_NetworkVariant%" value="0" />

  <!-- 
    0 = Disabled / Not allowed - only reserved
    1 = LAN
    2 = WLAN
  -->
  <op:config name="%NET_NetworkPreselect%" value="1" />

  <!--
    Prepare: Not ready to use
  -->
  <op:config name="%NET_ServiceHTTP%" value="0" />
  <op:config name="%NET_ServiceNTP%" value="0" />

  <ManufacturerData>
    <Manufacturer>
      <ApplicationPrograms>
        <ApplicationProgram>
          <Static>
            <ParameterTypes>
              <ParameterType Id="%AID%_PT-CustomHostname" Name="CustomHostname">
                <TypeRestriction Base="Value" SizeInBit="1">
                  <Enumeration Text="Standard" Value="0" Id="%ENID%" />
                  <Enumeration Text="Anpassen" Value="1" Id="%ENID%" />
                </TypeRestriction>
              </ParameterType>
              <ParameterType Id="%AID%_PT-DHCP" Name="DHCP">
                <TypeRestriction Base="Value" SizeInBit="1">
                  <Enumeration Text="Ja" Value="0" Id="%ENID%" />
                  <Enumeration Text="Nein" Value="1" Id="%ENID%" />
                </TypeRestriction>
              </ParameterType>
              <ParameterType Id="%AID%_PT-HostAddress" Name="HostAddress">
                <TypeIPAddress AddressType="HostAddress" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-SubnetMask" Name="SubnetMask">
                <TypeIPAddress AddressType="SubnetMask" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-GatewayAddress" Name="GatewayAddress">
                <TypeIPAddress AddressType="GatewayAddress" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-Hostname" Name="Hostname" ValidationErrorRef="%AID%_M-%TT%00001">
                <TypeText SizeInBit="192" Pattern="^[A-Za-z](?:[A-Za-z0-9\-]*[A-Za-z0-9])*$"/>
              </ParameterType>
              <ParameterType Id="%AID%_PT-WifiSSID" Name="WifiSSID">
                <TypeText SizeInBit="256" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-WifiPassword" Name="WifiPassword">
                <TypeText SizeInBit="504" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-MacAddress" Name="MacAddress" ValidationErrorRef="%AID%_M-%TT%00002">
                <TypeText SizeInBit="136" Pattern="^(?:[0-9A-Fa-f]{2}:){5}(?:[0-9A-Fa-f]{2})$" />
              </ParameterType>
              <ParameterType Id="%AID%_PT-LanMode" Name="LanMode">
                <TypeRestriction Base="Value" SizeInBit="4">
                  <Enumeration Text="Automatisch" Value="0" Id="%ENID%" />
                  <Enumeration Text="10 MBit/s (Stromsparender)" Value="3" Id="%ENID%" />
                  <Enumeration Text="100 MBit/s" Value="1" Id="%ENID%" />
                  <!-- <Enumeration Text="100 MBit/s Halb-Duplex" Value="2" Id="%ENID%" /> -->
                  <!-- <Enumeration Text="10 MBit/s Halb-Duplex" Value="4" Id="%ENID%" /> -->
                </TypeRestriction>
              </ParameterType>
              <ParameterType Id="%AID%_PT-NetworkType" Name="NetworkType">
                <TypeRestriction Base="Value" SizeInBit="4">
                  <Enumeration Text="Disabled" Value="0" Id="%ENID%" />
                  <Enumeration Text="LAN" Value="1" Id="%ENID%" />
                  <Enumeration Text="WLAN" Value="2" Id="%ENID%" />
                </TypeRestriction>
              </ParameterType>
              <ParameterType Id="%AID%_PT-NetworkTypeSelect" Name="NetworkTypeSelect">
                <!-- 0 is reserved for disabled -->
                <TypeRestriction Base="Value" SizeInBit="4">
                  <Enumeration Text="LAN" Value="1" Id="%ENID%" />
                  <Enumeration Text="WLAN" Value="2" Id="%ENID%" />
                </TypeRestriction>
              </ParameterType>
              <ParameterType Id="%AID%_PT-NetworkVariant" Name="NetworkVariant">
                <TypeNumber SizeInBit="8" Type="unsignedInt" minInclusive="%NET_NetworkVariant%" maxInclusive="%NET_NetworkVariant%" />
              </ParameterType>
            </ParameterTypes>
            <Parameters>
              <!-- helper to check against a certain value in the case of a choose -->
              <Parameter Id="%AID%_P-%TT%01000" Name="SelectHelper"          ParameterType="%AID%_PT-ValueDpt5"         Text=""        Value="0" Access="None" />

              <Parameter Id="%AID%_P-%TT%00061" Name="NetworkTypeSelect"     ParameterType="%AID%_PT-NetworkTypeSelect" Text="Adapter" Value="%NET_NetworkPreselect%" />
              <Parameter Id="%AID%_P-%TT%00062" Name="NetworkVariant"        ParameterType="%AID%_PT-NetworkVariant"    Text=""        Value="%NET_NetworkVariant%" Access="None" />

              <!-- IP Config -->
              <Union SizeInBit="128"><!-- 16bytes-->
                <Memory CodeSegment="%MID%" Offset="0" BitOffset="0" />
                <Parameter Id="%AID%_UP-%TT%00011" Name="HostAddress"        ParameterType="%AID%_PT-HostAddress"        Offset="0"  BitOffset="0" Text="IP-Adresse" Value="192.168.0.1" />
                <Parameter Id="%AID%_UP-%TT%00012" Name="SubnetMask"         ParameterType="%AID%_PT-SubnetMask"         Offset="4"  BitOffset="0" Text="Subnetzsmaske" Value="255.255.255.0" />
                <Parameter Id="%AID%_UP-%TT%00013" Name="GatewayAddress"     ParameterType="%AID%_PT-GatewayAddress"     Offset="8"  BitOffset="0" Text="Standardgateway" Value="192.168.0.254" />
                <Parameter Id="%AID%_UP-%TT%00021" Name="NameserverAddress"  ParameterType="%AID%_PT-HostAddress"        Offset="12" BitOffset="0" Text="Nameserver" Value="192.168.0.254" />
                <!-- Reserved for Nameserver 2 -->
              </Union>

              <!-- Wifi -->
              <Parameter Id="%AID%_P-%TT%00101" Name="SelectWifiConfig"      ParameterType="%AID%_PT-CheckBox"           Text="" Value="0" Access="None"/>
              <Union SizeInBit="760"><!-- 95 -->
                <Memory CodeSegment="%MID%" Offset="16" BitOffset="0" />
                <Parameter Id="%AID%_UP-%TT%00041" Name="WifiSSID"           ParameterType="%AID%_PT-WifiSSID"           Offset="0"  BitOffset="0" Text="SSID" Value="" />
                <Parameter Id="%AID%_UP-%TT%00042" Name="WifiPassword"       ParameterType="%AID%_PT-WifiPassword"       Offset="32"  BitOffset="0" Text="Passswort" Value="" />
              </Union>

              <!-- 1bit options -->
              <Union SizeInBit="8">
                <Memory CodeSegment="%MID%" Offset="111" BitOffset="0" />
                <Parameter Id="%AID%_UP-%TT%00001" Name="CustomHostname"     ParameterType="%AID%_PT-OnOffYesNo"         Offset="0" BitOffset="0" Text="Hostname anpassen" Value="0" />
                <Parameter Id="%AID%_UP-%TT%00002" Name="StaticIP"           ParameterType="%AID%_PT-DHCP"               Offset="0" BitOffset="1" Text="DHCP" Value="0" />
                <Parameter Id="%AID%_UP-%TT%00003" Name="CustomMacAddress"   ParameterType="%AID%_PT-OnOffYesNo"         Offset="0" BitOffset="2" Text="MAC-Adresse anpassen" Value="0" />
                <!-- 5 bits free -->
              </Union>

              <!-- Services -->
              <Union SizeInBit="8">
                <Memory CodeSegment="%MID%" Offset="112" BitOffset="0" />
                <Parameter Id="%AID%_UP-%TT%00031" Name="mDNS"               ParameterType="%AID%_PT-OnOffYesNo"         Offset="0" BitOffset="0" Text="mDNS" Value="1" /> 
                <Parameter Id="%AID%_UP-%TT%00032" Name="HTTP"               ParameterType="%AID%_PT-OnOffYesNo"         Offset="0" BitOffset="1" Text="Weberver" Value="1" /> 
                <Parameter Id="%AID%_UP-%TT%00033" Name="NTP"                ParameterType="%AID%_PT-OnOffYesNo"         Offset="0" BitOffset="2" Text="Zeitgeber (NTP)" Value="0" /> 
                <!-- 5 bits free -->
              </Union>

              <!-- Hostname -->
              <Parameter Id="%AID%_P-%TT%00010" Name="HostName"              ParameterType="%AID%_PT-Hostname"           Text="Hostname" Value="" >
                <Memory CodeSegment="%MID%" Offset="113" BitOffset="0" />
                <!-- 192bits / 24bytes-->
              </Parameter>

              <!-- MacAddress -->
              <Parameter Id="%AID%_P-%TT%00051" Name="MacAddress"            ParameterType="%AID%_PT-MacAddress"         Text="MAC-Adresse" Value="" >
                <Memory CodeSegment="%MID%" Offset="137" BitOffset="0" />
                <!-- 136bits / 17bytes-->
              </Parameter>

              <!-- Type & Mode -->
              <Union SizeInBit="8">
                <Memory CodeSegment="%MID%" Offset="154" BitOffset="0" />
                <Parameter Id="%AID%_UP-%TT%00052" Name="LanMode"            ParameterType="%AID%_PT-LanMode"       Offset="0" BitOffset="0" Text="LAN-Modus" Value="0" />
                <Parameter Id="%AID%_UP-%TT%00060" Name="NetworkType"        ParameterType="%AID%_PT-NetworkType"   Offset="0" BitOffset="4" Text="Typ" Value="0" Access="None" />
              </Union>

              <Union SizeInBit="128">
                <Memory CodeSegment="%MID%" Offset="155" BitOffset="0" />
                <!-- Set dummy for last Byte to reserve Space -->
                <Parameter Id="%AID%_UP-%TT%00099" Name="Dummy"  ParameterType="%AID%_PT-ValueDpt5" Offset="15" BitOffset="0" Text="" Value="0" />
              </Union>
              
            </Parameters>
            <ParameterRefs>
              <ParameterRef Id="%AID%_P-%TT%01000_R-%TT%0100000" RefId="%AID%_P-%TT%01000" />
              <ParameterRef Id="%AID%_UP-%TT%00001_R-%TT%0000101" RefId="%AID%_UP-%TT%00001" />
              <ParameterRef Id="%AID%_UP-%TT%00002_R-%TT%0000201" RefId="%AID%_UP-%TT%00002" />
              <ParameterRef Id="%AID%_UP-%TT%00003_R-%TT%0000301" RefId="%AID%_UP-%TT%00003" />
              <ParameterRef Id="%AID%_P-%TT%00010_R-%TT%0001001" RefId="%AID%_P-%TT%00010" />
              <ParameterRef Id="%AID%_UP-%TT%00011_R-%TT%0001101" RefId="%AID%_UP-%TT%00011" />
              <ParameterRef Id="%AID%_UP-%TT%00012_R-%TT%0001201" RefId="%AID%_UP-%TT%00012" />
              <ParameterRef Id="%AID%_UP-%TT%00013_R-%TT%0001301" RefId="%AID%_UP-%TT%00013" />
              <ParameterRef Id="%AID%_UP-%TT%00021_R-%TT%0002101" RefId="%AID%_UP-%TT%00021" />
              <ParameterRef Id="%AID%_UP-%TT%00031_R-%TT%0003101" RefId="%AID%_UP-%TT%00031" />
              <ParameterRef Id="%AID%_UP-%TT%00032_R-%TT%0003201" RefId="%AID%_UP-%TT%00032" />
              <ParameterRef Id="%AID%_UP-%TT%00033_R-%TT%0003301" RefId="%AID%_UP-%TT%00033" />
              <ParameterRef Id="%AID%_UP-%TT%00041_R-%TT%0004101" RefId="%AID%_UP-%TT%00041" />
              <ParameterRef Id="%AID%_UP-%TT%00042_R-%TT%0004201" RefId="%AID%_UP-%TT%00042" />
              <ParameterRef Id="%AID%_P-%TT%00051_R-%TT%0005101" RefId="%AID%_P-%TT%00051" />
              <ParameterRef Id="%AID%_UP-%TT%00052_R-%TT%0005201" RefId="%AID%_UP-%TT%00052" />

              <!-- NetworkType -->
              <ParameterRef Id="%AID%_UP-%TT%00060_R-%TT%0006000" RefId="%AID%_UP-%TT%00060" /><!-- NetworkType / Hidden -->
              <ParameterRef Id="%AID%_P-%TT%00061_R-%TT%0006100" RefId="%AID%_P-%TT%00061" /><!-- UserSelect / LAN -->
              <ParameterRef Id="%AID%_P-%TT%00062_R-%TT%0006200" RefId="%AID%_P-%TT%00062" /><!-- NetworkVariant / Hidden -->
            </ParameterRefs>
            <ComObjectTable>
            </ComObjectTable>
            <ComObjectRefs>
            </ComObjectRefs>
            <Messages>
              <Message Id="%AID%_M-%TT%00001" Name="InvalidHostName" Text="Darf nur 24 Zeichen lang sein und muss aus Buchstaben, Zahlen und Bindestriche bestehen." />
              <Message Id="%AID%_M-%TT%00002" Name="InvalidMacAddress" Text="Die MAC-Adresse muss im Format XX:XX:XX:XX:XX:XX und hexadezimal eingegeben werden." />
            </Messages>
          </Static>
          <Dynamic>
            <ChannelIndependentBlock>
              <ParameterBlock Id="%AID%_PB-nnn" Name="Network" Text="Netzwerk" Icon="ethernet" HelpContext="Empty">
                <!-- Hidden -->
                <ParameterRefRef RefId="%AID%_P-%TT%01000_R-%TT%0100000" />
                <ParameterRefRef RefId="%AID%_UP-%TT%00060_R-%TT%0006000" />
                <ParameterRefRef RefId="%AID%_P-%TT%00062_R-%TT%0006200" />

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Netzwerk" UIHint="Headline" />

                <!-- NetworkType based on NetwortVariant config -->
                <choose ParamRefId="%AID%_P-%TT%00062_R-%TT%0006200">
                  <when test="0">
                    <ParameterRefRef RefId="%AID%_P-%TT%00061_R-%TT%0006100" IndentLevel="1" HelpContext="NET-NetworkType" />
                    <choose ParamRefId="%AID%_P-%TT%00061_R-%TT%0006100">
                      <when test="1"><Assign TargetParamRefRef="%AID%_UP-%TT%00060_R-%TT%0006000" Value="1"/></when>
                      <when test="2"><Assign TargetParamRefRef="%AID%_UP-%TT%00060_R-%TT%0006000" Value="2"/></when>
                    </choose>
                  </when>
                  <when test="1"><Assign TargetParamRefRef="%AID%_UP-%TT%00060_R-%TT%0006000" Value="1"/></when>
                  <when test="2"><Assign TargetParamRefRef="%AID%_UP-%TT%00060_R-%TT%0006000" Value="2"/></when>
                </choose>

                <choose ParamRefId="%AID%_P-%TT%01000_R-%TT%0100000"><!-- Static 0 -->
                  <when test="%NET_IPConfigTool%"> <!-- default -->
                    <ParameterRefRef RefId="%AID%_UP-%TT%00002_R-%TT%0000201" IndentLevel="1" HelpContext="NET-IpAddress" />
                    <choose ParamRefId="%AID%_UP-%TT%00002_R-%TT%0000201">
                      <when test="1">
                        <ParameterRefRef RefId="%AID%_UP-%TT%00011_R-%TT%0001101" IndentLevel="1" HelpContext="NET-IpAddress" />
                        <ParameterRefRef RefId="%AID%_UP-%TT%00012_R-%TT%0001201" IndentLevel="1" HelpContext="NET-IpAddress" />
                        <ParameterRefRef RefId="%AID%_UP-%TT%00013_R-%TT%0001301" IndentLevel="1" HelpContext="NET-IpAddress" />
                        <ParameterRefRef RefId="%AID%_UP-%TT%00021_R-%TT%0002101" IndentLevel="1" HelpContext="NET-IpAddress" />
                      </when>
                    </choose>
                  </when>
                  <when test="!=%NET_IPConfigTool%"> <!-- router -->
                    <ParameterSeparator Id="%AID%_PS-nnn" Text="IP-Einstellungen können im Eigenschaften Feld der ETS, Tab IP gesetzt werden." UIHint="Information" />
                  </when>
                </choose>

                <choose ParamRefId="%AID%_UP-%TT%00060_R-%TT%0006000">
                  <when test="2">
                    <ParameterSeparator Id="%AID%_PS-nnn" Text="WLAN" UIHint="Headline" />
                    <ParameterRefRef RefId="%AID%_UP-%TT%00041_R-%TT%0004101" IndentLevel="1" HelpContext="NET-Wifi" />
                    <ParameterRefRef RefId="%AID%_UP-%TT%00042_R-%TT%0004201" IndentLevel="1" HelpContext="NET-Wifi" />
                  </when>
                </choose>

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Services" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_UP-%TT%00031_R-%TT%0003101" IndentLevel="1" HelpContext="NET-MDNS" />
                
                <!-- HTTP -->
                <choose ParamRefId="%AID%_P-%TT%01000_R-%TT%0100000">
                  <when test="!=%NET_ServiceHTTP%">
                    <ParameterRefRef RefId="%AID%_UP-%TT%00032_R-%TT%0003201" IndentLevel="1" />
                    <choose ParamRefId="%AID%_UP-%TT%00032_R-%TT%0003201">
                      <when test="1">
                        <ParameterBlock Id="%AID%_PB-nnn" Name="HTTP" Text="Webserver" Icon="web" HelpContext="Empty">
                          <ParameterSeparator Id="%AID%_PS-nnn" Text="Webservereinstellung" UIHint="Headline" />
                        </ParameterBlock>
                      </when>
                    </choose>
                  </when>
                </choose>

                <!-- NTP -->
                <choose ParamRefId="%AID%_P-%TT%01000_R-%TT%0100000">
                  <when test="!=%NET_ServiceNTP%">
                    <ParameterRefRef RefId="%AID%_UP-%TT%00033_R-%TT%0003301" IndentLevel="1" />
                    <choose ParamRefId="%AID%_UP-%TT%00033_R-%TT%0003301">
                      <when test="0">
                        <Assign TargetParamRefRef="%AID%_P-1000010_R-100001001" Value="0" />
                      </when>
                      <when test="1">
                        <Assign TargetParamRefRef="%AID%_P-1000010_R-100001001" Value="1" />
                        <ParameterBlock Id="%AID%_PB-nnn" Name="NTP" Text="Zeitgeber" Icon="clock-outline" HelpContext="Empty">
                          <ParameterSeparator Id="%AID%_PS-nnn" Text="Zeitgeber (NTP)" UIHint="Headline" />
                        </ParameterBlock>
                      </when>
                    </choose>
                  </when>
                </choose>

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Erweiterte Einstellung" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_UP-%TT%00001_R-%TT%0000101" IndentLevel="1" HelpContext="NET-Hostname" />
                <choose ParamRefId="%AID%_UP-%TT%00001_R-%TT%0000101">
                  <when test="1">
                    <ParameterRefRef RefId="%AID%_P-%TT%00010_R-%TT%0001001" IndentLevel="2" HelpContext="NET-Hostname" />
                  </when>
                </choose>

                <ParameterRefRef RefId="%AID%_UP-%TT%00003_R-%TT%0000301" IndentLevel="1" HelpContext="NET-MacAddress" />
                <choose ParamRefId="%AID%_UP-%TT%00003_R-%TT%0000301">
                  <when test="1">
                    <ParameterRefRef RefId="%AID%_P-%TT%00051_R-%TT%0005101" IndentLevel="2" HelpContext="NET-MacAddress" />
                  </when>
                </choose>

                <choose ParamRefId="%AID%_UP-%TT%00060_R-%TT%0006000">
                  <when test="1">
                    <ParameterRefRef RefId="%AID%_UP-%TT%00052_R-%TT%0005201" IndentLevel="1" HelpContext="NET-LanMode" />
                  </when>
                </choose>

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Standardmäßig werden aus der Seriennummer der Hostname (OpenKNX-XXXXXXXX), sowie die MAC-Adresse (5E:84:XX:XX:XX:XX) gebaut. Diese können hier angepasst werden." UIHint="Information" />

              </ParameterBlock>
            </ChannelIndependentBlock>
          </Dynamic>
        </ApplicationProgram>
      </ApplicationPrograms>
      <Baggages>
        <Baggage TargetPath="" Name="Help_de.zip" Id="%FILE-HELP-de%">
          <FileInfo TimeInfo="%DATETIME%" />
        </Baggage>
        <Baggage TargetPath="" Name="Icons.zip" Id="%FILE-ICONS%">
          <FileInfo TimeInfo="%DATETIME%" />
        </Baggage>
      </Baggages>
    </Manufacturer>
  </ManufacturerData>
